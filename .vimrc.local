let s:paste_enable = 0
function! s:TogglePaste()
    echohl ModeMsg
    if s:paste_enable == 0
        let s:paste_enable = 1
        exe "set paste"
        echo "Paste is Enabled."
    else
        let s:paste_enable = 0
        exe "set nopaste"
        echo "Paste is DISABLE."
