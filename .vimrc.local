set mouse=a
let s:mouse_enable = 1
function! s:ToggleMouse()
    echohl ModeMsg
    if s:mouse_enable == 0
        let s:mouse_enable = 1
        exe "set mouse=" . "a"
        echo "Mouse is Enabled."
    else
        let s:mouse_enable = 0
        exe "set mouse="
        echo "Mouse is DISABLE."
    endif
    echohl Normal
endfunction
command ToggleMouse :call s:ToggleMouse()
nnoremap <unique> <leader><leader>m :ToggleMouse<CR>
